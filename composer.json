{
    "name": "talalalmrka/fgx",
    "description": "Reusable UI Components for Laravel by fadgram",
    "keywords": [
        "fgx",
        "fadgram",
        "laravel",
        "livewire",
        "components",
        "ui"
    ],
    "version": "1.0.8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "talalalmrka",
            "email": "talalminfo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.5.3",
        "spatie/laravel-ray": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "Fgx\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fgx\\Providers\\FgxServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}