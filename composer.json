{
    "name": "talalalmrka/fgx",
    "description": "Reusable UI Components for Laravel by fadgram",
    "keywords": [
        "fgx",
        "fadgram",
        "laravel",
        "livewire",
        "components",
        "ui"
    ],
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "talalalmrka",
            "email": "talalminfo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "symfony/console": "^6.0|^7.0",
        "livewire/livewire": "^3.5.19",
        "laravel/prompts": "^0.1|^0.2|^0.3",
        "spatie/laravel-medialibrary": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Fgx\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fgx\\Providers\\FgxServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}